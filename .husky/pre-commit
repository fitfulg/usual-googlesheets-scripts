#!/bin/sh
. "$(dirname "$0")/_/husky.sh"

# Ejecuta lint-staged y captura el c贸digo de salida
npx lint-staged
exit_code=$?

# Si lint-staged falla, imprime un mensaje y termina con un c贸digo de error
if [ $exit_code -ne 0 ]; then
  echo "lint-staged fall贸 con el c贸digo de salida $exit_code"
  exit $exit_code
fi
